<% 
    if (typeof(titleColor) === 'undefined') var titleColor;
    titleColor = titleColor || '#545454';

    if (typeof(titleTextColor) === 'undefined') var titleTextColor;
    titleTextColor = titleTextColor || 'white';

    if (typeof(bodyColor) === 'undefined') var bodyColor;
    bodyColor = bodyColor || '#007EB1'; 

    if (typeof(bodyTextColor) === 'undefined') var bodyTextColor;
    bodyTextColor = bodyTextColor || 'white'; 
-%>
<%- input.renderBadge({ title, titleColor, titleTextColor, body, bodyColor, bodyTextColor }) %>